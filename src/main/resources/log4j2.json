{
  "Configutation": {
    "Appenders": {
      "Console": {
        "name": "Console",
        "target": "SYSTEM_ERR",
        "PatternLayout": {
          "pattern": "[%-5level] %d{MMM dd, yyyy hh:mm:ss}: %msg%n"
        }
      }
    },
    "Loggers": {
      "Logger": {
        "name": "org.apache.thrift.transport",
        "level": "ERROR"
      },
      "Root": {
        "level": "info",
        "AppenderRef": [
          {
            "ref": "Console"
          }
        ]
      }
    }
  }
}